pkgname=besic-relay
pkgver=0.1.0
pkgrel=1
pkgdesc="BESI-C relay setup"
arch=("any")
depends=("bash" "besic-lib" "besic-heartbeat" "besic-s3upload" "besic-beacon")
conflicts=("besic-basestation")

package() {
  mkdir -p "$pkgdir/etc/besic"

  cp "$srcdir/type.conf" "$pkgdir/etc/besic/type.conf"
}
