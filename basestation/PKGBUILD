pkgname=besic-basestation
pkgver=0.1.0
pkgrel=1
pkgdesc="BESI-C basestation setup"
arch=("any")
depends=("bash" "besic-lib" "besic-remote")
conflicts=("besic-relay")

package() {
  mkdir -p "$pkgdir/etc/besic"

  cp "$srcdir/type.conf" "$pkgdir/etc/besic/type.conf"
}
